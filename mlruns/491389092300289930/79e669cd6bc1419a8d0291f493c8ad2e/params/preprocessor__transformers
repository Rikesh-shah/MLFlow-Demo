[('age', Pipeline(steps=[('impute', SimpleImputer(strategy='median')),
                ('scale', StandardScaler())]), ['age']), ('fare', Pipeline(steps=[('scale', StandardScaler())]), ['fare']), ('embarked', Pipeline(steps=[('impute', SimpleImputer(strategy='most_frequent')),
                ('encode',
                 OneHotEncoder(handle_unknown='ignore', sparse_output=False))]), ['embarked']), ('sex', OneHotEncoder(handle_unknown='ignore', sparse_output=False), ['sex']), ('family', MinMaxScaler(), ['family'])]